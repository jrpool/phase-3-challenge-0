{
  "name": "phase-3-challenge",
  "description": "Learners Guild tasks for phase-3 sorting interview",
  "author": "Jonathan Pool",
  "license": "MIT",
  "private": false,
  "version": "0.0.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": ">=6.24.1",
    "babel-preset-env": ">=1.5.2",
    "babel-preset-latest": ">=6.24.1",
    "babel-register": ">=6.24.1",
    "chai": ">=4.0.2",
    "eslint": "^4.0.0",
    "mocha": ">=3.4.2"
  },
  "scripts": {
    "lint1":
      "./node_modules/eslint/bin/eslint.js part-1/src/ part-1/test/; exit 0",
    "lintfix1":
      "./node_modules/eslint/bin/eslint.js --fix part-1/src/ part-1/test/; exit 0",
    "test1": "mocha --compilers js:babel-register part-1/test/*.js",
    "lint2":
      "./node_modules/eslint/bin/eslint.js part-2/src/ part-2/test/; exit 0",
    "lintfix2":
      "./node_modules/eslint/bin/eslint.js --fix part-2/src/ part-2/test/; exit 0",
    "test2": "mocha --compilers js:babel-register part-2/test/*.js",
    "lint3":
      "./node_modules/eslint/bin/eslint.js part-3/src/ part-3/test/; exit 0",
    "lintfix3":
      "./node_modules/eslint/bin/eslint.js --fix part-3/src/ part-3/test/; exit 0",
    "test3": "mocha --compilers js:babel-register part-3/test/*.js"
  },
  "bugs": {
    "url": "https://github.com/jrpool/phase-3-challenge.git/issues"
  },
  "homepage": "https://github.com/jrpool/phase-3-challenge.git/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrpool/phase-3-challenge.git"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-array-constructor": [
        "error"
      ],
      "no-cond-assign": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "no-const-assign": [
        "error"
      ],
      "no-new-object": [
        "error"
      ],
      "no-var": [
        "error"
      ],
      "prefer-const": [
        "error"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
